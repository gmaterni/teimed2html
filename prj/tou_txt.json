{
  "exe_dir": {
    "dir": "xml/tou",
    "pattern": "*.xml",
    "par_subst": ".xml|",
    "par_name": "$F",
    "exe_file": [
      [
        "teixml2html.py",
        "-i xml/tou/$F.xml",
        "-o html/tou/txt/$F.html",
        "-c cnf/tou_dipl_txt.json",
        "-wa w"
      ],
      [
        "writehtml.py ",
        "-o html/tou/txt/$F.html",
        "-wa a"
      ],
      [
        "teixml2html.py",
        "-i xml/tou/$F.xml",
        "-o html/tou/txt/$F.html",
        "-c cnf/tou_inter_txt.json",
        "-wa a"
      ]
    ]
  }
}