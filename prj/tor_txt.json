{
  "exe_dir": {
    "dir": "xml/tor",
    "pattern": "*.xml",
    "par_subst": ".xml|",
    "par_name": "$F",
    "exe_file": [
      [
        "teixml2html.py",
        "-i xml/tor/$F.xml",
        "-o html/tor/txt/$F.html",
        "-c cnf/tor_dipl_txt.json",
        "-wa w",
        "-d 0"
      ],
      ["writehtml.py -o html/tor/txt/$F.html -wa a"],
      [
        "teixml2html.py",
        "-i xml/tor/$F.xml",
        "-o html/tor/txt/$F.html",
        "-c cnf/tor_inter_txt.json",
        "-wa a",
        "-d 0"
      ]
    ]
  }
}
