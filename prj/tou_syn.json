{
  "exe_dir": {
    "dir": "xml/tou",
    "pattern": "*.xml",
    "par_subst": ".xml|",
    "par_name": "$F",
    "exe_file": [
      [
        "writehtml.py",
        "-o html/tou/syn/$F.html -wa w",
        "-i '</div><div id=\"tou_int_id\" class=\"text_pannel tei_int\">'"
      ],
      [
        "teixml2html.py",
        "-i xml/tou/$F.xml",
        "-o html/tou/syn/$F.html -wa a",
        "-c cnf/tou_dipl_syn.json",
        "-d 0"
      ],
      [
        "writehtml.py",
        "-o html/tou/syn/$F.html -wa a",
        "-i '</div><div id=\"tou_int_id\" class=\"text_pannel tei_int\">'"
      ],
      [
        "teixml2html.py",
        "-i xml/tou/$F.xml",
        "-o html/tou/syn/$F.html -wa a",
        "-c cnf/tou_inter_syn.json",
        "-d 0"
      ],
      [
        "writehtml.py",
        "-o html/tou/syn/$F.html -wa a",
        "-i '</div>'"
      ]
    ]
  }
}