{
  "exe_dir": {
    "dir": "xml/par",
    "pattern": "*ep12.xml",
    "par_subst": ".xml|",
    "par_name": "$F",
    "exe_file": [
      [
        "teixml2html.py",
        "-i xml/par/$F.xml",
        "-o html/par/syn/$F_s.html",
        "-c cnf/par_dipl_syn.json",
        "-wa w",
        "-d 1"
      ]
    ]
  }
}