{
  "exe_dir": {
    "dir": "xml/ven",
    "pattern": "*.xml",
    "par_subst": ".xml|",
    "par_name": "$F",
    "exe_file": [
      [
        "writehtml.py",
        "-o html/ven/syn/$F.html",
        "-i '<div id=\"ven_int_id\" class=\"text_pannel tei_int\">'",
        "-wa w"
      ],
      [
        "teixml2html.py",
        "-i xml/ven/$F.xml",
        "-o html/ven/syn/$F.html",
        "-c cnf/ven_dipl_syn.json",
        "-wa a"
      ],
      [
        "writehtml.py",
        "-o html/ven/syn/$F.html",
        "-i '</div><div id=\"ven_int_id\" class=\"text_pannel tei_int\">'",
        "-wa a"
      ],
      [
        "teixml2html.py",
        "-i xml/ven/$F.xml",
        "-o html/ven/syn/$F.html",
        "-c cnf/ven_inter_syn.json",
        "-wa a"
      ],
      [
        "writehtml.py",
        "-o html/ven/syn/$F.html",
        "-i '</div>'",
        "-wa a"
      ]
    ]
  }
}