{
  "exe_dir": {
    "dir": "xml/par",
    "pattern": "*.xml",
    "par_subst": ".xml|",
    "par_name": "$F",
    "exe_file": [
      [
        "writehtml.py",
        "-o html/par/syn/$F.html",
        "-i '<div id=\"par_int_id\" class=\"text_pannel tei_int\">'",
        "-wa w"
      ],
      [
        "teixml2html.py",
        "-i xml/par/$F.xml",
        "-o html/par/syn/$F.html",
        "-c cnf/par_dipl_syn.json",
        "-wa a"
      ],
      [
        "writehtml.py",
        "-o html/par/syn/$F.html",
        "-i '</div><div id=\"par_int_id\" class=\"text_pannel tei_int\">'",
        "-wa a"
      ],
      [
        "teixml2html.py",
        "-i xml/par/$F.xml",
        "-o html/par/syn/$F.html",
        "-c cnf/par_inter_syn.json",
        "-wa a"
      ],
      [
        "writehtml.py",
        "-o html/par/syn/$F.html",
        "-i '</div>'",
        "-wa a"
      ]
    ]
  }
}