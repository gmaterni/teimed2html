{
    "exe": [
        "teixml2html.py -i xml/tou/tou_list.xml -o html/tou/syn/tou_listd.html -t cnf/htmldipl.csv -c cnf/cnf_episode_list.json",
        "teixml2html.py -i xml/tou/tou_list.xml -o html/tou/syn/tou_listi.html -t cnf/htmlinter.csv -c cnf/cnf_episode_list.json"
