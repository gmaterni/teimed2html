{
  "exe_dir": {
    "dir": "xml/par",
    "pattern": "*.xml",
    "par_subst": ".xml|",
    "par_name": "$F",
    "exe_file": [
      [
        "teixml2html.py",
        "-i xml/par/$F.xml",
        "-o html/par/txt/$F.html",
        "-c cnf/par_dipl_txt.json",
        "-wa w",
        "-d 0"
      ],
      ["writehtml.py -o html/par/txt/$F.html -wa a"],
      [
        "teixml2html.py",
        "-i xml/par/$F.xml",
        "-o html/par/txt/$F.html",
        "-c cnf/par_inter_txt.json",
        "-wa a",
        "-d 0"
      ]
    ]
  }
}
