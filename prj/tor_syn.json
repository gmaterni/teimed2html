{
  "exe_dir": {
    "dir": "xml/tor",
    "ptrn": "*.xml",
    "par_sub": ".xml|",
    "par_name": "$F",
    "prog": [
      "teixml2html_di_tmpl.py",
      "-i xml/tor/$F.xml",
      "-o html/tor/syn/$F.html",
      "-t html/syn_episode.html",
      "-cd cnf/tor_dipl_syn.json",
      "-ci cnf/tor_inter_syn.json",
      "-d 0"
    ]
  }
}
