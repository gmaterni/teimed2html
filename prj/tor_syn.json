{
  "exe_dir": {
    "dir": "xml/tor",
    "pattern": "*.xml",
    "par_subst": ".xml|",
    "par_name": "$F",
    "exe_file": [
      [
        "writehtml.py",
        "-o html/tor/syn/$F.html -wa w",
        "-i '</div><div id=\"tor_int_id\" class=\"text_pannel tei_int\">'"
      ],
      [
        "teixml2html.py",
        "-i xml/tor/$F.xml",
        "-o html/tor/syn/$F.html -wa a",
        "-c cnf/tor_dipl_syn.json",
        "-d 0"
      ],
      [
        "writehtml.py",
        "-o html/tor/syn/$F.html -wa a",
        "-i '</div><div id=\"tor_int_id\" class=\"text_pannel tei_int\">'"
      ],
      [
        "teixml2html.py",
        "-i xml/tor/$F.xml",
        "-o html/tor/syn/$F.html -wa a",
        "-c cnf/tor_inter_syn.json",
        "-d 0"
      ],
      [
        "writehtml.py",
        "-o html/tor/syn/$F.html -wa a",
        "-i '</div>'"
      ]
    ]
  }
}