{
  "exe_dir": {
    "dir": "xml/tor",
    "pattern": "*.xml",
    "par_subst": ".xml|",
    "par_name": "$F",
    "exe_file": [
      [
        "writehtml.py",
        "-o html/tor/syn/$F.html",
        "-i '<div id=\"tor_int_id\" class=\"text_pannel tei_int\">'",
        "-wa w"
      ],
      [
        "teixml2html.py",
        "-i xml/tor/$F.xml",
        "-o html/tor/syn/$F.html",
        "-c cnf/tor_dipl_syn.json",
        "-wa a"
      ],
      [
        "writehtml.py",
        "-o html/tor/syn/$F.html",
        "-i '</div><div id=\"tor_int_id\" class=\"text_pannel tei_int\">'",
        "-wa a"
      ],
      [
        "teixml2html.py",
        "-i xml/tor/$F.xml",
        "-o html/tor/syn/$F.html",
        "-c cnf/tor_inter_syn.json",
        "-wa a"
      ],
      [
        "writehtml.py",
        "-o html/tor/syn/$F.html",
        "-i '</div>'",
        "-wa a"
      ]
    ]
  },
  "remove_dir": [
    {
      "dir": "html/tor/syn",
      "pattern": "tor_list.html"
    }
  ]
}