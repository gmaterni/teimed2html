{
  "log": "0",
  "exe_dir": {
    "dir": "xml/tou1",
    "pattern": "*.xml",
    "par_subst": ".xml|",
    "par_name": "$F",
    "exe_file": [
      [
        "teixml2html.py",
        "-i xml/tou1/$F.xml",
        "-o html/tou1/txt/$F.html",
        "-c prj_cfg/tou1_dipl_txt.json",
        "-wa w "
      ],
      [
        "writehtml.py ",
        "-o html/tou1/txt/$F.html",
        "-wa a"
      ],
      [
        "teixml2html.py",
        "-i xml/tou1/$F.xml",
        "-o html/tou1/txt/$F.html",
        "-c prj_cfg/tou1_inter_txt.json",
        "-wa a "
      ]
    ]
  }
}